pod 'CocoaLumberjack', '~> 1.8.1'
pod 'HockeySDK', '~> 3.5.5'
pod 'MMPopLabel', :git => 'https://github.com/hbridge/MMPopLabel.git'
pod 'CRToast', '~> 0.0.7'
pod 'Localytics-iOS-Client', '~> 2.23.0'
pod 'FormKit.m', '~> 0.4.0'
pod 'SAMGradientView', '~> 0.2.1'
pod 'LKbadgeView', '~> 1.1.0'
pod 'LiveFrost', '~> 1.1.2'
pod 'WYPopoverController', '~> 0.2.2'
pod 'SVProgressHUD', :git => 'https://github.com/samvermette/SVProgressHUD.git'